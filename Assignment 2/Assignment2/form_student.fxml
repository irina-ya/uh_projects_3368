                     <TableColumn fx:id="col_1" prefWidth="50.0" text="Id">
                         <cellValueFactory>
                             <PropertyValueFactory property="id" />
                         </cellValueFactory>
                     </TableColumn>
                     <TableColumn fx:id="col_2" prefWidth="175.0" text="Name">
                         <cellValueFactory>
